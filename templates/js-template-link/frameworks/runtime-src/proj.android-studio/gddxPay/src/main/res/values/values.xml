    <style name="vertical_scrollbar">
        <item name="android:scrollbarAlwaysDrawVerticalTrack">false</item>
        <item name="android:scrollbarStyle">outsideInset</item>
        <item name="android:scrollbars">vertical</item>
        <item name="android:fadeScrollbars">true</item>
        <item name="android:scrollbarSize">1000dp</item>
        <item name="android:scrollbarFadeDuration">1000</item>
        <item name="android:scrollbarTrackVertical">@drawable/egame_sdk_tv_scroll_bar_bg</item>
        <item name="android:scrollbarThumbVertical">@drawable/egame_sdk_tv_scroll_bar</item>
