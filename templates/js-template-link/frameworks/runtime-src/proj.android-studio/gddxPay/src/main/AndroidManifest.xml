<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.iflytek.payComponent">

    <application android:allowBackup="true" android:label="@string/app_name"
        android:supportsRtl="true">
        <!-- sdk debug开关 -->
        <meta-data
            android:name="EGAME_SDK_DEBUG"
            android:value="true" />
        <!--
            &lt;!&ndash; 配置游戏的渠道号 本平台填10010000 &ndash;&gt;
        <meta-data
            android:name="egame_channel_id"
            android:value="" />
        &lt;!&ndash; 配置游戏的GameId &ndash;&gt;
        <meta-data
            android:name="EGAME_LAUNCH_GAMEID"
            android:value="" />
        -->
        <!-- 配置游戏的操作方式 -->
        <meta-data
            android:name="EGAME_LAUNCH_CONTROL"
            android:value="1" />
        <!--
            <meta-data
            android:name="EGAME_LAUNCH_ACTIVITY"
            android:value="cn.egame.egametvsdkdemo.MainActivity" />
        -->

        <activity
            android:name="cn.egame.terminal.sdk.tv.launch.EgameTvLaunchActivity"
            android:screenOrientation="landscape"
            android:theme="@style/egame_launch_transparent" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="cn.egame.terminal.sdk.pay.tv.entrance.EgamePayView"
            android:configChanges="navigation|keyboardHidden|keyboard"
            android:exported="true"
            android:screenOrientation="landscape"
            android:theme="@style/egame_Transparent" />
        <activity
            android:name="cn.egame.terminal.sdk.pay.tv.EgamePayViewCore"
            android:configChanges="navigation|keyboardHidden|keyboard"
            android:exported="true"
            android:screenOrientation="landscape"
            android:theme="@style/egame_Transparent" />
        <activity
            android:name="cn.egame.terminal.sdk.pay.tv.activity.EgameAidouPaidActivity"
            android:configChanges="navigation|keyboardHidden|keyboard"
            android:exported="true"
            android:screenOrientation="landscape"
            android:theme="@style/egame_Transparent" />
        <activity
            android:name="cn.egame.terminal.sdk.pay.tv.activity.webview.EgameBrowserActivity"
            android:configChanges="navigation|keyboardHidden|keyboard"
            android:exported="true"
            android:screenOrientation="landscape"
            android:theme="@style/egame_Transparent" />
        <activity
            android:name="cn.egame.terminal.sdk.pay.tv.activity.EgameCompletePersonDataActivity"
            android:configChanges="navigation|keyboardHidden|keyboard"
            android:exported="true"
            android:screenOrientation="landscape"
            android:theme="@style/egame_Transparent" />
        <activity
            android:name="cn.egame.terminal.sdk.pay.tv.activity.EgamePaidAmountActivity"
            android:configChanges="navigation|keyboardHidden|keyboard"
            android:exported="true"
            android:screenOrientation="landscape"
            android:theme="@style/egame_Transparent" />
        <activity
            android:name="cn.egame.terminal.sdk.pay.tv.activity.wxpay.WxPayActivity"
            android:configChanges="navigation|keyboardHidden|keyboard"
            android:exported="true"
            android:screenOrientation="landscape"
            android:theme="@style/egame_Transparent" />
        <activity
            android:name="cn.egame.terminal.sdk.pay.tv.activity.alipay.AlipayMainActivity"
            android:configChanges="navigation|keyboardHidden|keyboard"
            android:exported="true"
            android:screenOrientation="landscape"
            android:theme="@style/egame_Transparent" />
        <activity
            android:name="cn.egame.terminal.sdk.pay.tv.activity.easybaby.EasyBabyInputInfoActivity"
            android:configChanges="navigation|keyboardHidden|keyboard"
            android:exported="true"
            android:screenOrientation="landscape"
            android:theme="@style/egame_Transparent" />
        <activity
            android:name="cn.egame.terminal.sdk.pay.tv.activity.EgameVoiceFee"
            android:configChanges="navigation|keyboardHidden|keyboard"
            android:exported="true"
            android:screenOrientation="landscape"
            android:theme="@style/egame_Transparent" />
    </application>

</manifest>
